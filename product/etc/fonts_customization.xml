<fonts-modification version="1">
        <family customizationType="new-named-family" name="SFDIN">
                <font weight="300" style="normal">SFDIN-Light.otf</font>
                <font weight="400" style="normal">SFDIN-Regular.otf</font>
                <font weight="500" style="normal">SFDIN-Medium.otf</font>
                <font weight="700" style="normal">SFDIN-Bold.otf</font>
        </family>

        <alias name="SFDIN-light" to="SFDIN" weight="300" />
        <alias name="SFDIN-Regular" to="SFDIN" weight="400" />
        <alias name="SFDIN-medium" to="SFDIN" weight="500" />
        <alias name="SFDIN-bold" to="SFDIN" weight="700" />

        <family customizationType="new-named-family" name="SFDINCondensed">
                <font weight="500" style="normal" postScriptName="SFDIN-CondensedBold">SFDINCondensed-Bold.otf</font>
        </family>
        <alias name="SFDINCondensed-bold" to="SFDINCondensed" weight="500" />

        <family customizationType="new-named-family" name="SFPRO">
                <font weight="400" style="normal">SFPRO-Regular.otf</font>
                <font weight="500" style="normal">SFPRO-Medium.otf</font>
                <font weight="700" style="normal">SFPRO-Bold.otf</font>
        </family>

        <alias name="SFPRO-Regular" to="SFPRO" weight="400" />
        <alias name="SFPRO-medium" to="SFPRO" weight="500" />
        <alias name="SFPRO-bold" to="SFPRO" weight="700" />

        <family customizationType="new-named-family" name="SFPROCondensed">
                <font weight="300" style="normal">SFPRO-SemiCondensedLight.otf</font>
                <font weight="500" style="normal">SFPRO-SemiCondensedBold.otf</font>
        </family>

        <alias name="SFPROCondensed-light" to="SFPROCondensed" weight="300" />
        <alias name="SFPROCondensed-bold" to="SFPROCondensed" weight="500" />

        <family customizationType="new-named-family" name="FlymeNumber">
                <font weight="100" style="normal" postScriptName="FlymeNumber-Regular">FlymeNumber-VF.ttf
                        <axis tag="wght" stylevalue="250"/>
                </font>
                <font weight="300" style="normal" postScriptName="FlymeNumber-Regular">FlymeNumber-VF.ttf
                        <axis tag="wght" stylevalue="300"/>
                </font>
                <font weight="400" style="normal" postScriptName="FlymeNumber-Regular">FlymeNumber-VF.ttf
                        <axis tag="wght" stylevalue="400"/>
                </font>
                <font weight="500" style="normal" postScriptName="FlymeNumber-Regular" >FlymeNumber-VF.ttf
                        <axis tag="wght" stylevalue="500"/>
                </font>
                <font weight="700" style="normal" postScriptName="FlymeNumber-Regular">FlymeNumber-VF.ttf
                        <axis tag="wght" stylevalue="700"/>
                </font>
                <font weight="900" style="normal" postScriptName="FlymeNumber-Regular">FlymeNumber-VF.ttf
                        <axis tag="wght" stylevalue="900"/>
                </font>
        </family>

        <alias name="FlymeNumber-light" to="FlymeNumber" weight="300" />
        <alias name="FlymeNumber-Regular" to="FlymeNumber" weight="400" />
        <alias name="FlymeNumber-medium" to="FlymeNumber" weight="500" />
        <alias name="FlymeNumber-bold" to="FlymeNumber" weight="700" />
</fonts-modification>